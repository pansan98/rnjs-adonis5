{
  "name": "adonis5",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "dev-serve": "nodemon ace serve",
    "dev-webpack": "webpack --progress --config webpack.dev.js --watch"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/preset-react": "^7.22.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@popperjs/core": "^2.11.8",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.7",
    "adonis-preset-ts": "^2.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "bootstrap": "^4.6.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^10.3.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.0",
    "postcss": "^8.4.28",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.2",
    "pure-react-carousel": "^1.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loader-spinner": "^5.3.4",
    "react-nl2br": "^1.0.4",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "sortablejs": "^1.15.0",
    "typescript": "~4.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "^5.9.0",
    "youch": "^3.2.3",
    "youch-terminal": "^2.2.2"
  },
  "dependencies": {
    "@adonisjs/application": "^5.3.0",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/fold": "^8.2.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/shield": "^7.1.1",
    "@adonisjs/view": "^6.2.0",
    "axios": "^1.4.0",
    "buffer-from": "^1.1.2",
    "camel-case": "^4.1.2",
    "change-case": "^4.1.2",
    "jsonfile": "^6.1.0",
    "luxon": "^3.4.1",
    "mysql2": "^3.6.0",
    "pascal-case": "^3.1.2",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
